&FORCE_EVAL
  METHOD QS
  &DFT
    &TRANSPORT
      TRANSPORT_METHOD TRANSMISSION
      ENERGY_INTERVAL 0.0
      N_KPOINTS 192
      NUM_POLE 48
      TASKS_PER_ENERGY_POINT 1
      TASKS_PER_POLE 1
      INJECTION_METHOD BEYN
      EPS_LIMIT 5.0E-2
      EPS_DECAY 1.0E-6
      &CONTACT
       N_ATOMS 6
       INJECTION_SIGN POSITIVE
      &END
      &CONTACT
       N_ATOMS 6
       INJECTION_SIGN NEGATIVE
      &END
      LINEAR_SOLVER MUMPS
      MATRIX_INVERSION_METHOD PEXSI
!      &PRINT
!       &CURRENT
!        STRIDE 1 1 1
!       &END
!      &END
    &END
    BASIS_SET_FILE_NAME BASIS_ZIJLSTRA
    POTENTIAL_FILE_NAME POTENTIAL 
    &MGRID
      CUTOFF 200
      &RS_GRID
         MAX_DISTRIBUTED_LEVEL 5
      &END
      SKIP_LOAD_BALANCE_DISTRIBUTED
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-10
      LS_SCF
      &DISTRIBUTION
        BASIC_OPTIMIZATION .FALSE.
        BASIC_SPATIAL_OPTIMIZATION .TRUE.
      &END
    &END QS
    &LS_SCF
      MAX_SCF    0
      EPS_SCF    1.0E-12
      REPORT_ALL_SPARSITIES OFF
      S_PRECONDITIONER NONE
      PURIFICATION_METHOD TRS4
    &END
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 10 10 39.296
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME gnr.xyz
      COORDINATE XYZ
      CONNECTIVITY off
    &END
    &KIND C
      BASIS_SET 3SP-GTH
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND H
      BASIS_SET 3SP-GTH
      POTENTIAL GTH-PBE-q1
    &END KIND
    &PRINT
      &ATOMIC_COORDINATES OFF
      &END
      &MOLECULES OFF
      &END
    &END
  &END SUBSYS
&END FORCE_EVAL
&MOTION
 &GEO_OPT
 &END
&END
&GLOBAL
  PROJECT gnr
  RUN_TYPE ENERGY
  PRINT_LEVEL MEDIUM
  &TIMINGS
     THRESHOLD 0.000001
  &END
  EXTENDED_FFT_LENGTHS
!TRACE
!TRACE_MASTER .FALSE.
!TRACE_MAX 1000
&END GLOBAL

